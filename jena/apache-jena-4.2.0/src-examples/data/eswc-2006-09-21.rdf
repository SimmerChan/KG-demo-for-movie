  <owl:Class rdf:ID="Presenter">
    <rdfs:subClassOf rdf:resource="#Role"/>
    <rdfs:label>Presenter</rdfs:label>
  </owl:Class>

  <owl:Class rdf:ID="Reviewer">
    <rdfs:subClassOf rdf:resource="#Role"/>
