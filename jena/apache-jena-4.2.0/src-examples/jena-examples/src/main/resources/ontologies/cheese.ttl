    owl:sameAs fb:food.cheese.texture ;
    .

:CheeseProduction 
    rdf:type rdfs:Class ;
    rdfs:label "type of production"@en ;
    rdfs:label "tipo di produzione"@it ;
    .

:FreshCheese
